{
  "extends": "airbnb",
  "env": {
		"browser": true,
		"node": true,
		"jest": true,
		"es6": true
	},
  "parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
  },
  "rules": {
    "react/jsx-filename-extension": [
      1, 
      { 
        "extensions": [
          ".js", 
          ".jsx"
        ] 
      }
    ],
    "react/forbid-prop-types": 0
  }
}
